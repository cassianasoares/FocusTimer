<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_app"
    app:startDestination="@id/timerFragment">

    <fragment
        android:id="@+id/timerFragment"
        android:name="com.demo.android.cassianass.focustimer.ui.fragment.TimerFragment"
        android:label="fragment_timer"
        tools:layout="@layout/fragment_timer" >
        <action
            android:id="@+id/action_timerFragment_to_setTimeFragment"
            app:destination="@id/setTimeFragment" />
    </fragment>
    <dialog
        android:id="@+id/setTimeFragment"
        android:name="com.demo.android.cassianass.focustimer.ui.fragment.SetTimeFragment"
        android:label="fragment_set_time"
        tools:layout="@layout/fragment_set_time" />
</navigation>